<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wxc.adplatform.dao.MerchantDao">
	
	<insert id="addMerchantRecord">
		insert into t_merchant_record(merchant_account,user_id,finance_id,device_code,owner,count,amount,create_time,statistics_time)
		 values(#{merchant_account},#{user_id},#{finance_id},#{device_code},#{owner},#{count},#{amount},#{create_time},#{statistics_time})
	</insert>
	
</mapper>